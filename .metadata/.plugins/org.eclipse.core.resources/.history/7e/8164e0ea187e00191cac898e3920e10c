        super( testName );
    }

    /**
     * @return the suite of tests being tested
     */
    public static Test suite()
    {
        return new TestSuite( AppTest.class );
    }

    /**
     * Rigourous Test :-)
     * Test is correct gives green bar
     */
    public void testApp()
    {
        assertTrue( true );
    }
    
    /**
     * Rigourous Test :-)
     * Test is incorrect gives red bar
     */
    public void testApp2()
    {
        assertTrue( false );
    }
    
