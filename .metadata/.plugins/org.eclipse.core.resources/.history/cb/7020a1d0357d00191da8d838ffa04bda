package com.qa.Hospital;

import java.util.ArrayList;

public class Nurse {
	
	private String name;
	
	private static ArrayList<Nurse> nlist = new ArrayList<Nurse>();
//	list.add(patient1);
//	list.add(patient2);
	
	public Nurse(String name) {
		
		this.name = name;
		nlist.add(this);
		
	}
		
		public static ArrayList<Patient>  getList(){
			
			return nlist;
		}


		public String getName() {
			
			return name;
		}


}
