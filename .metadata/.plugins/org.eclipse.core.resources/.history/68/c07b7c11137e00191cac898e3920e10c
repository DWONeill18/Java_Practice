package com.qa.TerrainGame;

import java.util.ArrayList;


public class Feature {
	
	private String name;
	
	private static ArrayList<Feature> list = new ArrayList<Feature>();

//	list.add(4);
	
	public Feature(String name) {
		
		this.name = name;
	
		list.add(this);
		
		
	}
	
	public static ArrayList<Feature>  getList(){
		
		return list;
	}


	public String getName() {
		
		return name;
	}
	

}
